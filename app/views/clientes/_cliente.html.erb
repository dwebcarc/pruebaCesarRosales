<div id="<%= dom_id cliente %>">
  <p>
    <strong>Nombre:</strong>
    <%= cliente.nombre %>
  </p>

  <p>
    <strong>Apellidos:</strong>
    <%= cliente.apellidos %>
  </p>

  <p>
    <strong>Direccion:</strong>
    <%= cliente.direccion %>
  </p>

  <p>
    <strong>Telefono:</strong>
    <%= cliente.telefono %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= cliente.email %>
  </p>

</div>
